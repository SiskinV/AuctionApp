@using AuctionApp.Models.Database
@model List<Auction>

<div class="row">
    
    @if(Model==null || Model.Count()==0){
        <h3>No auctions!</h3>
    }else{
        @foreach (var item in Model)
        {  
            <div class="col-xl-3">
                @{ await Html.RenderPartialAsync("SingleAuction",item); }
            </div>
        }
    }
</div>